{
  "ReverseProxy": {
    "Routes": {
      "ui": {
        "ClusterId": "ui",
        "Match": { "Path": "{**catch-all}" },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "arc": { "ClusterId": "arc", "Match": { "Path": "/api/arc/{**catch-all}" } },
      "dar": { "ClusterId": "dar", "Match": { "Path": "/api/dar/{**catch-all}" } },
      "dba": { "ClusterId": "dba", "Match": { "Path": "/api/dba/{**catch-all}" } },
      "bud": { "ClusterId": "bud", "Match": { "Path": "/api/bud/{**catch-all}" } },
      "ctr": { "ClusterId": "ctr", "Match": { "Path": "/api/ctr/{**catch-all}" } },
      "cos": { "ClusterId": "cos", "Match": { "Path": "/api/cos/{**catch-all}" } },
      "rap": { "ClusterId": "rap", "Match": { "Path": "/api/rap/{**catch-all}" } },
      "rde": { "ClusterId": "rde", "Match": { "Path": "/api/rde/{**catch-all}" } },
      "rep": { "ClusterId": "rep", "Match": { "Path": "/api/rep/{**catch-all}" } },
      "rre": { "ClusterId": "rre", "Match": { "Path": "/api/rre/{**catch-all}" } },
      "rst": { "ClusterId": "rst", "Match": { "Path": "/api/rst/{**catch-all}" } },
      "elsa": { "ClusterId": "elsa", "Match": { "Path": "/workflow/{**catch-all}" } }
    },
    "Clusters": {
      "ui": { "Destinations": { "ui": { "Address": "http://ui" } } },
      "arc": { "Destinations": { "arc": { "Address": "http://arc" } } },
      "dar": { "Destinations": { "dar": { "Address": "http://dar" } } },
      "dba": { "Destinations": { "dba": { "Address": "http://dba" } } },
      "bud": { "Destinations": { "bud": { "Address": "http://bud" } } },
      "ctr": { "Destinations": { "ctr": { "Address": "http://ctr" } } },
      "cos": { "Destinations": { "cos": { "Address": "http://cos" } } },
      "rap": { "Destinations": { "rap": { "Address": "http://rap" } } },
      "rde": { "Destinations": { "rde": { "Address": "http://rde" } } },
      "rep": { "Destinations": { "rep": { "Address": "http://rep" } } },
      "rre": { "Destinations": { "rre": { "Address": "http://rre" } } },
      "rst": { "Destinations": { "rst": { "Address": "http://rst" } } },
      "elsa": { "Destinations": { "elsa": { "Address": "http://elsa" } } }
    }
  }
}
